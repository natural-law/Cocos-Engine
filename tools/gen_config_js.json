{
    "copy_config" : [
        {
            "from": "cocos2d-js/frameworks/cocos2d-html5",
            "to": "gen/cocos/frameworks/cocos2d-js/frameworks/cocos2d-html5"
        },
        {
            "from": "cocos2d-js/frameworks/js-bindings/external",
            "to": "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/external",
            "exclude": [
                "*linux",
                "*prebuilt/ios",
                "*prebuilt/mac"
            ]
        },
        {
            "from": "cocos2d-js/frameworks/js-bindings/bindings",
            "to": "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings",
            "exclude": [
                "*proj.*",
                "*.cpp",
                "*CMakeLists.txt"
            ]
        },
        {
            "from": "cocos2d-js/templates",
            "to": "gen/cocos/frameworks/cocos2d-js/templates",
            "exclude" : [
                "js-template-default/*"
            ]
        },
        {
            "from": "cocos2d-js/tools/cocos2d-console",
            "to": "gen/cocos/frameworks/cocos2d-js/tools/cocos2d-console",
            "exclude": [
                "config.json",
                "download-bin.py",
                "version.json",
                "*.pyc"
            ]
        },
        {
            "from": "js-templates",
            "to": "gen/cocos/frameworks/cocos2d-js/templates",
            "exclude": [
                "js-template-default/*",
                ".gitignore"
            ]
        }
    ],
    "android_mks" : [
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/manual/chipmunk/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/manual/extension/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/manual/localstorage/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/manual/network/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/manual/cocosbuilder/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/manual/ui/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/manual/cocostudio/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/manual/spine/Android.mk"
    ],
    "xcode_proj_info" : {
        "cocos2d-js/frameworks/js-bindings/bindings/proj.ios_mac/cocos2d_js_bindings.xcodeproj" : {
            "outputdir" : "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/prebuilt",
            "targets" : [ "jsbindings" ]
        }
    },
    "win32_proj_info" : {
        "cocos2d-js/build/cocos2d_jsb_samples.vc2012.sln" : {
            "outputdir" : "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/prebuilt/win32",
            "targets" : [
                "libJSBinding", "libJSBindingForBuilder", "libJSBindingForChipmunk",
                "libJSBindingForExtension", "libJSBindingForGui", "libJSBindingForLocalStorage",
                "libJSBindingForNetwork", "libJSBindingForSpine", "libJSBindingForStudio"
            ]
        }
    }
}
