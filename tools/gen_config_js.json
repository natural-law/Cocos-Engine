{
    "copy_config" : [
        {
            "from": "cocos2d-js/frameworks",
            "to": "gen/cocos/frameworks/cocos2d-js/frameworks",
            "include": [
                "cocos2d-html5/*",
                "js-bindings/external/*",
                "js-bindings/bindings/Android.mk",
                "js-bindings/bindings/auto/*.hpp",
                "js-bindings/bindings/manual/*.h",
                "js-bindings/bindings/manual/*/Android.mk"
            ]
        },
        {
            "from": "cocos2d-js/frameworks/js-bindings/cocos2d-x/cocos",
            "to": "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/cocos2d-x/cocos",
            "include": [
                "editor-support/*/Android.mk",
                "ui/Android.mk",
                "audio/android/Android.mk",
                "Android.mk",
                "network/Android.mk",
                "platform/android/Android.mk",
                "cocos2d.h",
                "2d/*.h$",
                "2d/*.props$",
                "3d/*.h$",
                "3d/*.inl",
                "audio/include/*.h",
                "base/*.h",
                "deprecated/*.h",
                "editor-support/*.h",
                "math/*.h",
                "math/*.inl",
                "network/*.h",
                "physics/*.h",
                "platform/*.h",
                "renderer/*.h",
                "storage/*.h$",
                "storage/local-storage/Android.mk",
                "ui/*.h"
            ]
        },
        {
            "from": "cocos2d-js/frameworks/js-bindings/cocos2d-x/extensions",
            "to": "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/cocos2d-x/extensions",
            "include": [
                "Android.mk",
                "cocos-ext.h",
                "ExtensionMacros.h",
                "assets-manager/*.h",
                "GUI/*.h",
                "physics-nodes/*.h"
            ]
        },
        {
            "from": "cocos2d-js/frameworks/js-bindings/cocos2d-x/external",
            "to": "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/cocos2d-x/external",
            "include": [
                "*/Android.mk",
                "curl/prebuilt/android/*.a",
                "curl/prebuilt/win32/*",
                "curl/include/*.h",
                "freetype2/prebuilt/android/*.a",
                "freetype2/prebuilt/win32/*",
                "freetype2/include/*.h",
                "jpeg/prebuilt/android/*.a",
                "jpeg/prebuilt/win32/*",
                "jpeg/include/*.h",
                "png/prebuilt/android/*.a",
                "png/prebuilt/win32/*",
                "png/include/*.h",
                "tiff/prebuilt/android/*.a",
                "tiff/prebuilt/win32/*",
                "tiff/include/*.h",
                "webp/prebuilt/android/*.a",
                "webp/prebuilt/win32/*",
                "webp/include/*.h",
                "websockets/prebuilt/android/*.a",
                "websockets/prebuilt/win32/*",
                "websockets/include/*.h",
                "Box2D/*.h",
                "chipmunk/include/*.h",
                "glfw3/include/*.h",
                "glfw3/prebuilt/win32/*",
                "json/*.h",
                "tinyxml2/*.h",
                "unzip/*.h",
                "xxhash/*.h",
                "win32-specific/*"
            ]
        },
        {
            "from": "cocos2d-js/frameworks/js-bindings/cocos2d-x/cocos/platform/android/java",
            "to": "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/cocos2d-x/cocos/platform/android/java"
        },
        {
            "from": "cocos2d-js/frameworks/js-bindings/bindings/script",
            "to": "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/script"
        },
        {
            "from": "cocos2d-js/templates",
            "to": "gen/cocos/frameworks/cocos2d-js/templates",
            "exclude" : [
                "js-template-default/*"
            ]
        },
        {
            "from": "cocos2d-js/tools/cocos2d-console",
            "to": "gen/cocos/frameworks/cocos2d-js/tools/cocos2d-console",
            "exclude": [
                "config.json",
                "download-bin.py",
                "version.json",
                "*.pyc"
            ]
        },
        {
            "from": "x-templates",
            "to": "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/cocos2d-x/templates",
            "include": "cocos2dx_files.json"
        },
        {
            "from": "js-templates",
            "to": "gen/cocos/frameworks/cocos2d-js/templates",
            "exclude": [
                "js-template-default/*",
                ".gitignore"
            ]
        }
    ],
    "android_mks" : [
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/cocos2d-x/external/Box2D/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/cocos2d-x/cocos/editor-support/cocosbuilder/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/cocos2d-x/extensions/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/cocos2d-x/cocos/ui/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/cocos2d-x/external/chipmunk/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/cocos2d-x/cocos/audio/android/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/cocos2d-x/cocos/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/cocos2d-x/cocos/editor-support/cocostudio/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/cocos2d-x/cocos/network/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/cocos2d-x/cocos/storage/local-storage/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/cocos2d-x/cocos/platform/android/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/cocos2d-x/cocos/editor-support/spine/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/manual/chipmunk/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/manual/extension/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/manual/localstorage/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/manual/network/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/manual/cocosbuilder/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/manual/ui/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/manual/cocostudio/Android.mk",
        "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/manual/spine/Android.mk"
    ],
    "xcode_proj_info" : {
        "cocos2d-js/frameworks/js-bindings/cocos2d-x/build/cocos2d_libs.xcodeproj" : {
            "outputdir" : "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/cocos2d-x/prebuilt",
            "targets" :[ "build all libs" ]
        },
        "cocos2d-js/frameworks/js-bindings/bindings/proj.ios_mac/cocos2d_js_bindings.xcodeproj" : {
            "outputdir" : "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/prebuilt",
            "targets" : [ "jsbindings" ]
        }
    },
    "win32_proj_info" : {
        "cocos2d-js/build/cocos2d_jsb_samples.vc2012.sln" : {
            "outputdir" : "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/cocos2d-x/prebuilt/win32",
            "targets" : [
                "libAudio", "libBox2D", "libchipmunk",
                "libcocos2d", "libCocosBuilder", "libCocosStudio",
                "libExtensions", "libGUI", "libLocalStorage",
                "libNetwork", "libSpine"
            ]
        },
        "cocos2d-js/build/cocos2d_jsb_samples.vc2012.sln" : {
            "outputdir" : "gen/cocos/frameworks/cocos2d-js/frameworks/js-bindings/bindings/prebuilt/win32",
            "targets" : [
                "libJSBinding", "libJSBindingForBuilder", "libJSBindingForChipmunk",
                "libJSBindingForExtension", "libJSBindingForGui", "libJSBindingForLocalStorage",
                "libJSBindingForNetwork", "libJSBindingForSpine", "libJSBindingForStudio"
            ]
        }
    }
}
