        "android_lib_name" : "cocos2dxandroid_static",
        "android_lib_file_name" : "libcocos2dandroid",
        "export_include" : [
            "../../"
        ],
        "export_ldlibs" : [
            "GLESv1_CM", "GLESv2", "EGL", "log", "z", "android"
        ]
    },
    "chipmunk" : {
